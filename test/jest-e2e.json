{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": ".",
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^(\\.{1,2}\\/(?:internal\\/.*|enums))\\.js$": "$1.ts",
    "^uuid$": "<rootDir>/mocks/uuid.ts"
  }
}
