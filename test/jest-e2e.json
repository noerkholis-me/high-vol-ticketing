{
  "moduleFileExtensions": ["js", "json", "ts"],
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": "test/.*\\.e2e-spec\\.ts$",
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "^(\\.{1,2}\\/(?:internal\\/.*|enums))\\.js$": "$1.ts",
    "^uuid$": "<rootDir>/test/mocks/uuid.ts"
  }
}
